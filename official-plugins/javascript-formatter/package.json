{
  "name": "javascript-formatter-plugin",
  "version": "1.0.0",
  "description": "JavaScript 代码格式化与压缩工具",
  "author": {
    "name": "UniHub Team",
    "email": "team@unihub.dev",
    "url": "https://github.com/unihub"
  },
  "keywords": [
    "javascript",
    "js",
    "format",
    "formatter",
    "prettier",
    "格式化"
  ],
  "type": "module",
  "scripts": {
    "dev": "vite --config ../_shared/vite.config.ts",
    "build": "vite build --config ../_shared/vite.config.ts",
    "package": "node ../_shared/scripts/package.js"
  },
  "unihub": {
    "id": "com.unihub.javascript-formatter",
    "name": "JavaScript 格式化",
    "icon": "https://api.iconify.design/vscode-icons/file-type-js-official.svg",
    "category": "formatter",
    "permissions": [
      "clipboard"
    ],
    "entry": "dist/index.html"
  },
  "dependencies": {
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "highlight.js": "^11.10.0",
    "lucide-vue-next": "^0.562.0",
    "prettier": "^3.4.2",
    "reka-ui": "^2.7.0",
    "tailwind-merge": "^3.4.0",
    "vue": "^3.5.13",
    "vue-sonner": "^2.0.9"
  },
  "devDependencies": {
    "@tailwindcss/vite": "^4.1.18",
    "@vitejs/plugin-vue": "^5.2.1",
    "@vueuse/core": "^14.1.0",
    "archiver": "^7.0.1",
    "tailwindcss": "^4.1.18",
    "tailwindcss-animate": "^1.0.7",
    "tw-animate-css": "^1.4.0",
    "typescript": "^5.7.3",
    "vite": "^6.0.3"
  }
}
